    "classnames": "^2.2.5",
    "normalize-scss": "^7.0.0",
    "react": "~15.4.0",
    "react-addons-css-transition-group": "~15.4.0",
    "react-dedux": "https://github.com/tiagomapmarques/react-dedux.git",
    "react-dom": "~15.4.0",
    "react-router": "~4.1.1",
