    "@types/react-router": "~4.1.1 || ~4.0.9",
    "@types/react-router-dom": "~4.1.1 || ~4.0.4",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.26.0",
    "file-loader": "^0.9.0",
    "node-sass": "^3.7.0",
    "postcss-cssnext": "~2.8.0",
    "postcss-each": "~0.9.3",
    "postcss-import": "~9.1.0",
    "postcss-loader": "~1.2.2",
    "postcss-mixins": "~5.4.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
