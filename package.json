  "license": "MIT",
  "repository": "https://github.com/tiagomapmarques/react-seed",
  "main": "src/app/index.tsx",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/app/index.html ./dist && copyfiles -u 1 \"./src/assets/**\" ./dist",
    "dist": "npm run clean && npm run copy && webpack --progress --bail --env dist -p",
    "serve:dev": "webpack-dev-server --env dev",
    "serve:dist": "webpack-dev-server --env dist -p --progress",
    "start": "npm run serve:dev"
  },
  "devDependencies": {
    "@types/react": "~15.4.0 || 15.0.24",
    "@types/react-dom": "~15.4.0 || 15.5.0",
    "@types/react-router": "^4.1.1 || 4.0.9",
    "@types/react-router-dom": "^4.1.1 || 4.0.4",
    "@types/requirejs": "^2.1.29",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.26.0",
    "file-loader": "^0.9.0",
    "node-sass": "^3.7.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-each": "^0.9.3",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-mixins": "^5.4.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.0",
    "typescript": "<2.3.0 || >2.3.2",
    "webpack-dev-server": "^2.1.0-beta"
  },
  "dependencies": {
    "react": "~15.4.0",
    "react-addons-css-transition-group": "~15.4.0",
    "react-dedux": "0.1.1-beta.1",
    "react-dom": "~15.4.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-toolbox": "^2.0.0-beta.8",
    "webpack": "^2.1.0-beta.6"
  }
}
